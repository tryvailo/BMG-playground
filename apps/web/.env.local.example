# Supabase Configuration (Local Development)
# Get these values by running: pnpm supabase:status
NEXT_PUBLIC_SUPABASE_URL=http://127.0.0.1:54321
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Site Configuration
NEXT_PUBLIC_SITE_URL=http://localhost:3000
NEXT_PUBLIC_PRODUCT_NAME=BoostMyGEO
NEXT_PUBLIC_SITE_TITLE=BoostMyGEO - Medical SEO Toolkit
NEXT_PUBLIC_SITE_DESCRIPTION=Comprehensive SEO toolkit for medical clinics

# Authentication Providers
# Password and Magic Link are enabled by default for local dev
# Set to 'false' to disable
NEXT_PUBLIC_AUTH_PASSWORD=true
NEXT_PUBLIC_AUTH_MAGIC_LINK=true
# OAuth providers (Google, etc.) - disabled by default
# Set to 'true' to enable
NEXT_PUBLIC_AUTH_OAUTH_ENABLED=false
